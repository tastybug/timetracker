CREATE TABLE project (uuid VARCHAR PRIMARY KEY, title VARCHAR NOT NULL, description VARCHAR DEFAULT NULL);
CREATE TABLE time_frame (uuid VARCHAR PRIMARY KEY, project_uuid INTEGER DEFAULT NULL, start_date VARCHAR DEFAULT NULL, end_date VARCHAR DEFAULT NULL, FOREIGN KEY(project_uuid) REFERENCES project(uuid) ON DELETE CASCADE DEFERRABLE INITIALLY DEFERRED);
CREATE TABLE project_time_constraints (uuid VARCHAR PRIMARY KEY, project_uuid INTEGER DEFAULT NULL, hour_limit DEFAULT NULL, start_date VARCHAR DEFAULT NULL, end_date VARCHAR DEFAULT NULL, FOREIGN KEY(project_uuid) REFERENCES project(uuid) ON DELETE CASCADE DEFERRABLE INITIALLY DEFERRED);